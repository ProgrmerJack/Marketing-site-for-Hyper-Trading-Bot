{
  "name": "marketing-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/next-with-wasm.mjs dev -p 3000",
    "dev:check": "node scripts/start-dev-and-check.mjs",
    "build": "node scripts/next-with-wasm.mjs build",
    "start": "node scripts/next-with-wasm.mjs start -p 3000",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:all": "npm run lint && npm run lint:css",
    "typecheck": "tsc --pretty --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "analyze": "node scripts/next-with-wasm.mjs build --analyze",
    "lighthouse": "lhci autorun --config=.lighthouse/lighthouserc.json",
    "lighthouse:build": "npm run build && lhci autorun --config=.lighthouse/lighthouserc.json",
    "ci:lighthouse": "lhci autorun",
    "ci": "npm run lint && npm run typecheck && npm run test && npm run ci:lighthouse",
    "prod:check:sse": "node scripts/test-sse-stream.mjs",
    "prod:check:gpc": "node scripts/test-gpc-compliance.mjs",
    "prod:check:a11y": "node scripts/test-accessibility-simple.mjs",
    "prod:check": "node scripts/production-readiness.mjs",
    "prod:check:local": "npm run prod:check http://localhost:3000",
    "check:assets": "node scripts/check-assets.mjs",
    "test:visual": "playwright test",
    "test:visual:prod": "cross-env ALLOW_UNSAFE_INLINE_SCRIPTS=true npm run build && cross-env ALLOW_UNSAFE_INLINE_SCRIPTS=true PLAYWRIGHT_USE_PROD_SERVER=true npx playwright test",
    "test:visual:update": "playwright test --update-snapshots",
    "test:cross-browser": "playwright test tests/cross-browser.spec.ts",
    "test:animations": "node scripts/test-animation-performance.mjs",
    "test:design": "npm run test:visual && npm run test:animations",
    "check:assets:prod": "node scripts/check-assets.mjs --url=",
    "dev:check:debug": "node scripts/start-dev-and-check.mjs --debug",
    "postinstall": "patch-package"
  },
  "dependencies": {
